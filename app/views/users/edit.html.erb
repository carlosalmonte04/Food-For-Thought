<h1>Sign up!</h1>
<%= flash[:message] %>
<%= form_for @user do |f| %><br><br>
<%= f.label :username %><br>
<%= f.text_field :username %><br><br>
<%= f.label :first_name %><br>
<%= f.text_field :first_name %><br><br>
<%= f.label :last_name %><br>
<%= f.text_field :last_name %><br><br>
<%= f.label :email %><br>
<%= f.text_field :email %><br><br>
<%= f.label :description %><br>
<%= f.text_area :description %><br><br>
<%= f.submit "Submit Changes" %>
<% end %>
